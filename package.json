{
  "name": "p2pmodels-wiki",
  "version": "0.0.1",
  "description": "A decentralized wiki for Aragon",
  "dependencies": {
    "@aragon/apps-shared-minime": "^1.0.1",
    "@aragon/apps-token-manager": "2.1.0",
    "@aragon/apps-voting": "2.1.0",
    "@aragon/os": "^4.2.1"
  },
  "devDependencies": {
    "@aragon/cli": "^5.9.7",
    "@aragon/wrapper": "5.0.0-rc.5",
    "@aragon/test-helpers": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "ethlint": "^1.2.4",
    "prettier": "^1.18.2",
    "solidity-coverage": "^0.6.2",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "start": "yarn run start:ipfs",
    "start:ipfs": "aragon run",
    "start:http": "aragon run --http localhost:8001 --http-served-from ./dist",
    "start:ipfs:template": "yarn run start:ipfs -- --template Template --template-init @ARAGON_ENS",
    "start:http:template": "yarn run start:http -- --template Template --template-init @ARAGON_ENS",
    "prepare": "cd app && yarn install && cd ..",
    "start:app": "cd app && yarn start && cd ..",
    "test": "cross-env TRUFFLE_TEST=true yarn run ganache-cli:test",
    "compile": "aragon contracts compile",
    "build": "cd app && yarn run build && cd ..",
    "publish:patch": "aragon apm publish patch",
    "publish:minor": "aragon apm publish minor",
    "publish:major": "aragon apm publish major",
    "versions": "aragon apm versions",
    "lint": "eslint . & solium --dir ./contracts",
    "lint:fix": "eslint . --fix & solium --dir ./contracts --fix",
    "coverage": "cross-env SOLIDITY_COVERAGE=true yarn run ganache-cli:test",
    "ganache-cli:test": "sh ./node_modules/@aragon/test-helpers/ganache-cli.sh"
  }
}
